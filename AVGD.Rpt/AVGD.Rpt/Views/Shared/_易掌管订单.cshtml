@{
    Layout = null;
}
<div style="text-align: left;">
    <form action="edsquery" method="post" id="queryfromid" class="form-inline" style="padding-left: 10px; padding-top: 9px;">
    <table>
        <tr>
            <td style="width:70px;">
                <span>订单编号:</span>
            </td>
            <td style="width:200px;">
                <input class="easyui-textbox" name="订单号" value="" />
            </td>
            <td style="width:70px;">
                <span>下单日期:</span>
            </td>
            <td style="width:200px;">
                <input class="easyui-datebox" name="下单日期1" value="" />
            </td>
            <td style="width:70px;">
                <span>至:</span>
            </td>
            <td style="width:200px;">
                <input class="easyui-datebox" name="下单日期2" value="" />
            </td>
        </tr>
        <tr>
            <td style="width:70px;">
                <span>商家品牌:</span>
            </td>
            <td style="width:200px;">
                <input class="easyui-textbox" name="商家品牌" value="" />
            </td>
            <td style="width:70px;">
                <span>经销商名:</span>
            </td >
            <td colspan="3" style="width:200px;">
                <input class="easyui-textbox" name="经销商名" value="" />
            </td>
        </tr>
        <tr>
            <td>订单状态:</td>
            <td colspan="5">
             <label style="padding-right:19px;margin-right: 0px;float: none;">
                    <input name="订单状态" type="checkbox" value="全部状态"  id="allStatus"/>全部状态
                </label> 
                <label style="padding-right:19px;margin-right: 0px;float: none;">
                    <input name="订单状态" type="checkbox" value="等待买家付款" />等待买家付款
                </label>
                <label style="padding-right:19px;margin-right: 0px;float: none;">
                    <input name="订单状态" type="checkbox" value="买家已付款" />买家已付款
                </label>
                <label style="padding-right:19px;margin-right: 0px;float: none;">
                    <input name="订单状态" type="checkbox" value="订单处理中" />订单处理中
                </label>
                <label style="padding-right:19px;margin-right: 0px;float: none;">
                    <input name="订单状态" type="checkbox" value="卖家已发货" />卖家已发货
                </label>
                <label style="padding-right:19px;margin-right: 0px;float: none;">
                    <input name="订单状态" type="checkbox" value="交易成功" />交易成功
                </label>
                <label  style="padding-right:19px;margin-right: 0px;float: none;">
                    <input name="订单状态" type="checkbox" value="交易关闭" />交易关闭
                </label>   
                <label style="padding-right:19px;margin-right: 0px;float: none;">
                    <input name="订单状态" type="checkbox" value="订单退款中" />订单退款中
                </label>
                <input type="hidden" name="report"  value="@ViewBag.report"/>
                @Html.AntiForgeryToken()
            </td>
        </tr>
    </table>
    <div>
    <a href="#" class="easyui-linkbutton" data-options="iconCls:'icon-search'" onclick="submitsquery('queryfromid')"
        style="padding: 18px; top: 70px; position: absolute; left: 890px;">查询</a>
           <a id="openwindow" href="#" class="easyui-linkbutton"  data-options="iconCls:'icon-search'" style="padding: 18px;top: 70px;position: absolute;left: 990px;"  >
                    高级</a>
                      <a id="" href="#" class="easyui-linkbutton" data-options="plain:'true'"  style="top: 133px;position: absolute;left: 950px;padding-left: 15px;padding-right: 15px;"  onclick="clearform('queryfromid')">
                    清空</a>
                    </div>
    </form>
</div>
